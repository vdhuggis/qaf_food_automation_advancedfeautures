<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Food Recipe Automation">
  <test name="BDD Test">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("Verify SAVES Button functionality")]]>
        </script>
      </method-selector>
    </method-selectors>
    <parameter name="search.properties" value="scenarios/metadataexample.feature"/>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- BDD Test -->
</suite> <!-- Food Recipe Automation -->
